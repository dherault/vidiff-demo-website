{
  "name": "vidiff-demo-website",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "edit": "echo 'Edit' >> README.md && git add . -A && git commit -m 'test commit' && git push",
    "sync-s3": "aws s3 sync build/ s3://dev.demo.vidiff.com --region eu-west-3",
    "invalidate-cloudfront": "aws cloudfront create-invalidation --distribution-id EZXIPA6K67NS9 --paths \"/*\"",
    "deploy": "npm run sync-s3 && npm run invalidate-cloudfront",
    "build-deploy": "npm run build && npm run deploy"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
